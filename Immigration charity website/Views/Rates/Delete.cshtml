@model Immigration_charity_website.Models.Rates

<h2>Delete Rating</h2>

<div>
    <h4>Are you sure you want to delete this rating?</h4>
    <dl class="dl-horizontal">
        <dt>User Name</dt>
        <dd>@Model.User.UserName</dd>
        <dt>Rating Value</dt>
        <dd>@Model.RatingValue</dd>
        <dt>Created At</dt>
        <dd>@Model.CreatedAt</dd>
    </dl>

    @using (Html.BeginForm("Delete", "Rates", FormMethod.Post, new { id = Model.Id }))
    {
        @Html.AntiForgeryToken()
        <input type="submit" value="Delete" class="btn btn-danger" />
        @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-secondary" })
    }
</div>
